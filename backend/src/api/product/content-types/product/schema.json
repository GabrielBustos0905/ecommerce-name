{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "decimal"
    },
    "brand": {
      "type": "enumeration",
      "enum": [
        "brand 1",
        "brand 2",
        "brand 3",
        "brend 4"
      ]
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "s",
        "m",
        "l",
        "xl"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    }
  }
}
